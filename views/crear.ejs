<%- include ("template/cabezera.ejs",{tituloweb:"Panel de administracion"})%>


<div class="container">
    <h2>Crear nuevo Ordenadores</h2>
    <form action="/ordenadores" class="my-2" method="POST">
    <input type="number" placeholder="Id" class="form-control my-2" name="id">
    <input type="text" placeholder="Nombre" class="form-control my-2" name="nombre">
    <input type="text" placeholder="Fabricante" class="form-control my-2" name="fabricante">
    <input type="text" placeholder="Año" class="form-control my-2" name="año">
    <input type="text" placeholder="SO" class="form-control my-2" name="so">
    <input type="text" placeholder="RAM" class="form-control my-2" name="ram">
    <button type="submit" class="btn btn-dark btn-block">
    Agregar
    </button>
    </form>
    </div>

